<html>
<head>
  <script src="/js/jquery-1.7.1.min.js"></script>
  <% if false %>
  <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/mustache.js/0.8.1/mustache.min.js"></script>
  <% end %>

  <script src="/js/master.js"></script>
   <script src="/js/production.js"></script>
  <link rel="stylesheet" type="text/css" href="/master.css">
  <link rel="icon" href="icon.png" >
<script id="template" type="x-tmpl-mustache">

  <svg xmlns="http://www.w3.org/2000/svg" version="1.1" width="100%" height="100%" id="svg" class="svg">
  <marker id="triangle"
        viewBox="0 0 5 10" refX="5" refY="5"
        markerUnits="strokeWidth"
        markerWidth="10" markerHeight="3"
        orient="auto">
        <path d="M 0 0 L 10 5 L 0 10 z" />
  </marker>
    <line x1="{{x1}}" y1="{{y1}}" x2="{{x2}}" y2="{{y2}}" class="arrow" marker-end="url(#triangle)"/>
  </svg>


</script>
</head>
<body >
    <%= yield %>
</body>
</html>
